{
  "expo": {
    "name": "higher-or-lower",
    "slug": "higher-or-lower",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/higherorlowerlogo.png",
    "scheme": "higherorlower",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nickkopel.higherorlower",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nickkopel.higherorlower"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/higherorlowerlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dc54cdca-8faa-4b39-860d-627e07783c06"
      }
    },
    "owner": "nickkopel"
  }
}
